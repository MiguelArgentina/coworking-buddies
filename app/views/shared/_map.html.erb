<% if place.persisted? %>
<div
  data-controller="leaflet-map"
  data-leaflet-map-latitude-value="<%= place.latitude %>"
  data-leaflet-map-longitude-value="<%= place.longitude %>"
  data-leaflet-map-name-value="<%= place.name %>"
  style="height: 400px;"
></div>
<% else %>
<div id="map" style="height: 400px;"></div>
<% end %>
